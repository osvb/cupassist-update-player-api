{
  "version": "2.0.0",
  "configurations": [
    {
      "name": "Launch Program",
      "type": "node",
      "request": "launch",
      "program": "${file}",
      "cwd": "${workspaceRoot}"
    },
    {
      "type": "node",
      "request": "attach",
      "name": "Attach to Process",
      "port": 5858
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Add Signups",
      "program": "${workspaceRoot}/src/index.js",
      "envFile": "${workspaceRoot}/.env",
      "stopOnEntry": false
    }
  ]
}
